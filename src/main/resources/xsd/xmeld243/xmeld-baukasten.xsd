         <xs:element name="BANKVERBINDUNG" type="xmeld:type.Bankverbindung"/>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="type.ZusatzfeldNachLandesrecht">
      <xs:annotation>
         <xs:appinfo>
            <title>Zusatzfelder nach Landesrecht</title>
         </xs:appinfo>
         <xs:documentation>Mit diesem Datentyp können zusätzliche nach Landesrecht zulässige Informationen mitgeteilt werden. Dieser Datentyp dient ausschließlich der Übermittlung von Informationen, die nicht im bundeseinheitlichen Teil des DSMeld definiert sind.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="feldname" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element kann ein zusätzliches Feld benannt werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="feldinhalt" maxOccurs="unbounded" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element kann der Wert des zusätzlichen Feldes benannt werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
