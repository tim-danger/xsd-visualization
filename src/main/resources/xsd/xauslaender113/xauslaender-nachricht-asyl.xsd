<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:xauslaender="http://www.osci.de/xauslaender1130"
           xmlns:xian="http://www.osci.de/xinneres/allgemeinername/2"
           xmlns:xiaz="http://www.osci.de/xinneres/azrnummer/1"
           xmlns:xida="http://www.osci.de/xinneres/datum/2"
           xmlns:xikomm="http://www.osci.de/xinneres/kommunikation/3"
           xmlns:xima="http://www.osci.de/xinneres/meldeanschrift/3"
           xmlns:xipa="http://www.osci.de/xinneres/postanschrift/3"
           xmlns:xoev-lc="http://xoev.de/latinchars/1_1/datatypes"
           targetNamespace="http://www.osci.de/xauslaender1130"
           version="1.13.0"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">
   <xs:annotation>
      <xs:appinfo>
         <standard>
            <nameLang>XAusländer</nameLang>
            <nameKurz>XAusländer</nameKurz>
            <nameTechnisch>xinneres.xauslaender</nameTechnisch>
            <kennung>urn:xoev-de:kosit:standard:xinneres.xauslaender</kennung>
            <beschreibung>Das Fachmodul XAusländer des Standards XInneres beschreibt die Datenübermittlung im Ausländerwesen.</beschreibung>
         </standard>
         <versionStandard>
            <version>1.13.0</version>
            <versionXOEVProfil>1.5.1</versionXOEVProfil>
            <versionXOEVHandbuch>2.1</versionXOEVHandbuch>
            <versionXGenerator>2.6.1</versionXGenerator>
            <versionModellierungswerkzeug>18.0 SP6</versionModellierungswerkzeug>
            <nameModellierungswerkzeug>MagicDraw</nameModellierungswerkzeug>
         </versionStandard>
      </xs:appinfo>
   </xs:annotation>
   <xs:include schemaLocation="xauslaender-allgemeine-datentypen.xsd"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/allgemeinername/2/xinneres-name.xsd"
              namespace="http://www.osci.de/xinneres/allgemeinername/2"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/azrnummer/1/xinneres-azrnummer.xsd"
              namespace="http://www.osci.de/xinneres/azrnummer/1"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/datum/2/xinneres-datum.xsd"
              namespace="http://www.osci.de/xinneres/datum/2"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/kommunikation/3/xinneres-kommunikation.xsd"
              namespace="http://www.osci.de/xinneres/kommunikation/3"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/meldeanschrift/3/xinneres-meldeanschrift.xsd"
              namespace="http://www.osci.de/xinneres/meldeanschrift/3"/>
   <xs:import schemaLocation="http://www.osci.de/xinneres/postanschrift/3/xinneres-postanschrift.xsd"
              namespace="http://www.osci.de/xinneres/postanschrift/3"/>
   <xs:import schemaLocation="http://xoev.de/latinchars/1_1/datatypes/latinchars.xsd"
              namespace="http://xoev.de/latinchars/1_1/datatypes"/>
   <xs:complexType name="Asyl.AussenstelleBAMF">
      <xs:annotation>
         <xs:appinfo>
            <title>Außenstelle des BAMF</title>
         </xs:appinfo>
         <xs:documentation>Mit diesem Datentyp werden Information zur einer Außenstelle oder einem Referat des bamf beschrieben.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="aussenstelleNummer" type="xauslaender:Code.Asyl.bAMFAussenstelle">
            <xs:annotation>
               <xs:documentation>Dieses Element bezeichnet die Dienststellennummer. Sie ist die eindeutige Behördenkennziffer (ohne Präfix) für die Außenstelle des bamf.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="aussenstelleBezeichnung"
                     minOccurs="0"
                     type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Dieses Element übermittelt die Kurzbezeichnung der Außenstelle des bamf.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="kontaktdaten"
                     maxOccurs="unbounded"
                     type="xikomm:Kommunikation">
            <xs:annotation>
               <xs:documentation>Mit diesem Element werden die Kontaktdaten des Ansprechpartners in der Außenstelle übermittelt. Dies kann auch eine zentrale Stelle sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="Asyl.BAMFAktenzeichen">
      <xs:annotation>
         <xs:appinfo>
            <title>BAMF-Aktenzeichen</title>
         </xs:appinfo>
         <xs:documentation>Dieses Element enthält das Aktenzeichen eines Asylverfahrens im BAMF.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xoev-lc:String.Latin"/>
   </xs:simpleType>
   <xs:complexType name="Asyl.Basisnachricht">
      <xs:annotation>
         <xs:documentation>Mit diesem Datentyp werden die Daten des Nachrichtenkopfs für alle Nachrichten im Kontext des Asylverfahrens übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xauslaender:Nachricht.G2G.XA">
            <xs:sequence>
               <xs:element name="organisationsbezeichnung"
                           minOccurs="0"
                           type="xoev-lc:String.Latin">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die Bezeichnung der bearbeitenden Stelle übermittelt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="bAMFAktenzeichen"
                           minOccurs="0"
                           type="xauslaender:Asyl.BAMFAktenzeichen"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Asyl.BasisnachrichtVonBAMF">
      <xs:annotation>
         <xs:documentation>Mit diesem Datentyp werden die Daten des Nachrichtenkopfs für alle Nachrichten vom bamf an andere Stellen im Kontext des Asylverfahrens übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xauslaender:Asyl.Basisnachricht">
            <xs:sequence>
               <xs:element name="aussenstelle" type="xauslaender:Asyl.AussenstelleBAMF">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die Außenstelle bzw. das zuständige Referat im bamf übermittelt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Asyl.Beschluss">
      <xs:annotation>
         <xs:appinfo>
            <title>Beschluss</title>
         </xs:appinfo>
         <xs:documentation>Dieser Datentyp enthält die Beschlussdaten zu einem Antrag.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="stattgegeben" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Dieses Element gibt an, ob dem Antrag stattgegeben wurde. Wenn einem Antrag nicht stattgegeben wurde, bedeutet das, dass er unanfechtbar abgelehnt wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datum" type="xida:Tagesdatum">
            <xs:annotation>
               <xs:documentation>Dieses Element gibt das Datum des Beschlusses an.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Asyl.Person">
      <xs:annotation>
         <xs:appinfo>
            <title>Daten zur Person</title>
         </xs:appinfo>
         <xs:documentation>Mit diesem Datentyp werden Personendaten beschrieben.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xauslaender:Asyl.PersonGrunddaten">
            <xs:sequence>
               <xs:element name="aZRNummer" type="xiaz:AZRNummer">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die AZR-Nummer übermittelt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="volkszugehoerigkeit"
                           minOccurs="0"
                           type="xauslaender:Volkszugehoerigkeit"/>
               <xs:element name="religion" minOccurs="0" type="xauslaender:Religion"/>
               <xs:element name="religionserfassungAZR" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird übermittelt, ob die Person der Erfassung ihrer Religionszugehörigkeit im AZR zugestimmt hat.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="sprache"
                           maxOccurs="2"
                           type="xauslaender:Code.Sprachenkatalog">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element werden Angaben zur Sprache der Person übermittelt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="herkunftsland" type="xauslaender:AngabenZumStaat">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird das Land übermittelt, in dem der Ausländer seinen letzten gewöhnlichen Aufenthalt hatte.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="aliaspersonalien" minOccurs="0" maxOccurs="unbounded">
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="familienname" type="xian:AllgemeinerName"/>
                        <xs:element name="vorname" type="xian:AllgemeinerName"/>
                        <xs:element name="geburt" type="xauslaender:Geburt"/>
                        <xs:element name="staatsangehoerigkeit"
                                    maxOccurs="4"
                                    type="xauslaender:AngabenZumStaat"/>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
               <xs:element name="ausweisdokument"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="xauslaender:Ausweisdokument"/>
               <xs:element name="sichtvermerkPassVisum" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Das Element gibt Auskunft darüber, ob ein Visum im Pass eingetragen ist.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fruehereAsylverfahren"
                           minOccurs="0"
                           type="xauslaender:Asyl.BAMFAktenzeichen">
                  <xs:annotation>
                     <xs:documentation>Sofern frühere Asylverfahren bekannt sind, ist das Aktenzeichen des ersten Asylverfahrens zu übermitteln.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="vertreter" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element werden Angaben zu einem Vertreter übermittelt.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="artDerVertretung" type="xauslaender:Code.Art.der.Vertretung">
                           <xs:annotation>
                              <xs:documentation>Hier werden die möglichen Arten der Vertretung definiert.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="aktenzeichen" minOccurs="0" type="xoev-lc:String.Latin">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird das Aktenzeichen des Vertreters übermittelt.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="vollmacht" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird angegeben, ob ein Nachweis der Bevollmächtigung vorgelegen hat.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="postempfangsbevollmaechtigt" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird angegeben, ob der Vertreter postempfangsbevollmächtigt ist. Es kann maximal einen Empfangsbevollmächtigten geben. Gibt es zwei Vertreter, von denen einer ein Rechtsanwalt ist, ist dieser als Postempfangsbevollmächtigter zu kennzeichnen.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="ansprechpartner" type="xauslaender:NameNatuerlichePerson">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird der Name des Vertreters übermittelt.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="erreichbarkeit"
                                    minOccurs="0"
                                    maxOccurs="unbounded"
                                    type="xikomm:Kommunikation">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element können Informationen zur Erreichbarkeit des Vertreters mitgeteilt werden.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="hausanschrift"
                                    minOccurs="0"
                                    type="xipa:PostalischeInlandsanschrift">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird die Hausanschrift des Vertreters übermittelt.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element name="postanschrift"
                                    minOccurs="0"
                                    type="xipa:PostalischeInlandsanschrift">
                           <xs:annotation>
                              <xs:documentation>Mit diesem Element wird die Postanschrift des Vertreters übermittelt.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Asyl.PersonGrunddaten">
      <xs:annotation>
         <xs:appinfo>
            <title>Grunddaten einer Person</title>
         </xs:appinfo>
         <xs:documentation>Dieser Datentyp beinhaltet die Grunddaten zu einer Person.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="familienname" type="xian:AllgemeinerName">
            <xs:annotation>
               <xs:documentation>Der aktuelle Familienname. Familienname kann der Geburtsname, der gemeinsam bestimmte Ehename oder Lebenspartnerschaftsname, der Ehename oder der Lebenspartnerschaftsname zusammen mit dem hinzugefügten Begleitnamen sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="geburtsname" minOccurs="0" type="xian:AllgemeinerName">
            <xs:annotation>
               <xs:documentation>Geburtsname ist der Name, der sich jeweils aus dem Geburtseintrag ergibt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="vorname" type="xian:AllgemeinerName">
            <xs:annotation>
               <xs:documentation>Vorname enthält einen Vornamen bzw. die Menge von Vornamen einer Person in der intendierten Reihenfolge.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="geburt" type="xauslaender:Geburt"/>
         <xs:element name="staatsangehoerigkeit"
                     maxOccurs="4"
                     type="xauslaender:AngabenZumStaat"/>
         <xs:element name="familienstand" type="xauslaender:Code.Familienstand">
            <xs:annotation>
               <xs:documentation>In diesem Feld wird die Information zum Familienstand oder einer entsprechenden Rechtsstellung bezüglich einer Lebenspartnerschaft in codierter Form abgelegt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="geschlecht" type="xauslaender:Geschlecht"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Asyl.PersonGrunddatenMitAZRNummer">
      <xs:annotation>
         <xs:appinfo>
            <title>Grunddaten einer Person inkl. AZR-Nummer</title>
         </xs:appinfo>
         <xs:documentation>Dieser Datentyp beinhaltet die Grunddaten zu einer Person inkl. AZR-Nummer.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="xauslaender:Asyl.PersonGrunddaten">
            <xs:sequence>
               <xs:element name="aZRNummer" minOccurs="0" type="xiaz:AZRNummer">
                  <xs:annotation>
                     <xs:documentation>Mit diesem Element wird die AZR-Nummer übermittelt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Asyl.SonstigeUnterlagen">
      <xs:annotation>
         <xs:appinfo>
            <title>Sonstige Unterlagen</title>
         </xs:appinfo>
         <xs:documentation>Dieser Datentyp enthält Informationen zu sonstigen Unterlagen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="typbezeichnung" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird der Typ des Dokuments übermittelt (z. B. Führerschein, Heiratsurkunde).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="identifikationsnummer"
                     minOccurs="0"
                     type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird eine identifizierende Angabe zum Dokument übermittelt (z. B. Seriennummer).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ausstellungsdatum" minOccurs="0" type="xida:Tagesdatum">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird das Ausstellungsdatum übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="gueltigBis" minOccurs="0" type="xida:Tagesdatum">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird das Gültigkeitsdatum übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="aussteller" minOccurs="0" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element werden Informationen zum Aussteller des Dokuments übermittelt (z. B. Innenministerium Syrien).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="einbehalten" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Dieses Element gibt an, ob das Dokument vom BAMF einbehalten wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:element name="Asyl.PersoenlicheErstantragstellung.110101">
      <xs:annotation>
         <xs:appinfo>
            <title>Persönliche Erstantragstellung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden die Daten aus einer persönlichen Erstantragstellung vom bamf an die zuständige abh (ABH/ZAB) und/oder ae (AE) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Antragstellung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="erstbelehrung" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die deutschsprachige Erstbelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="erstbelehrungFremdsprachig"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird eine fremdsprachige Erstbelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.SchriftlicheErstantragstellung.110102">
      <xs:annotation>
         <xs:appinfo>
            <title>Schriftliche Erstantragstellung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden die Daten aus einer schriftlichen Erstantragstellung vom bamf an die zuständige abh (ABH/ZAB) und/oder ae (AE) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum des Antragseingangs im bamf übermittelt (Eingangsstempel bzw. Datum des Faxempfangs).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Folgeantragstellung.110103">
      <xs:annotation>
         <xs:appinfo>
            <title>Asylfolgeantragstellung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird angezeigt, dass ein Ausländer die Durchführung eines weiteren Asylverfahrens beantragt hat. Die Nachricht wird vom bamf an die zuständige abh (ABH/ZAB) und/oder ae (AE) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum des persönlichen Antrags bzw. des Antragseingangs im bamf übermittelt (Eingangsstempel bzw. Datum des Faxempfangs).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="folgebelehrung" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die deutschsprachige Folgebelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="folgebelehrungFremdsprachig"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird eine fremdsprachige Folgebelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="letzteAnschriftInland"
                              minOccurs="0"
                              type="xima:Meldeanschrift">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die letzte, bekannte Wohnanschrift im Inland übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="wiederzuzugImAzr" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird gemeldet, ob die ABH/ZAB, als zuständige Behörde, den Wiederzuzug im AZR melden muss.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Fortfuehrungsantrag.110104">
      <xs:annotation>
         <xs:appinfo>
            <title>Wiederaufnahme des Verfahrens (Fortführungsantrag)</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird die Information übermittelt, dass das Verfahren beim bamf fortgeführt wird, nachdem die Bearbeitung des Asylgesuches eingestellt wurde. Das Bundesamt informiert die zuständige abh (ABH/ZAB) und/oder ae (AE) darüber, dass ein Asylverfahren fortgeführt wird.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsweiterfuehrungsart"
                              type="xauslaender:Code.Asyl.Fortfuehrungsantrag">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird übermittelt, auf welche Weise der Fortführungsantrag weiter bearbeitet wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.WiederaufgreifenAbschiebungsverbote.110105">
      <xs:annotation>
         <xs:appinfo>
            <title>Wiederaufgreifensantrag zu Abschiebungsverboten nach § 60 Abs. 5 oder 7 AufenthG</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird die Information übermittelt, dass der Ausländer einen Antrag auf Wiederaufgreifen des Verfahrens gem. § 51 VwVfG gestellt hat. Das bamf informiert die zuständige abh (ABH/ZAB).</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum des persönlichen Antrags bzw. des Antragseingangs im bamf übermittelt (Eingangsstempel bzw. Datum des Faxempfangs).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.AsylantragstellungKind.110106">
      <xs:annotation>
         <xs:appinfo>
            <title>Mitteilung über Asylantragstellung nach § 14a Abs. 1 bzw. Abs. 2 AsylG</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird der zuständigen abh (ABH/ZAB) und/oder ae (AE) mitgeteilt, dass das Bundesamt einen Asylantrag nach § 14a Abs. 1 bzw. 2 AsylG angelegt hat.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden und für die der Inhalt dieser Nachricht zutrifft.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Antragstellung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="erstbelehrung" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die deutschsprachige Erstbelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="erstbelehrungFremdsprachig"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird eine fremdsprachige Erstbelehrung gem. § 10 AsylG übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Prognosemeldung.110201">
      <xs:annotation>
         <xs:appinfo>
            <title>Prognosemeldung im Asylverfahren</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht informiert das BAMF die zuständige abh (ABH/ZAB) und/oder die ae (AE) darüber, ob im Falle einer vorliegenden Erstantragstellung eine kurzfristige Entscheidung wegen Unzulässigkeit oder offensichtlich unbegründeter Antragstellung gem. § 50 AsylG möglich ist. Oder sie informiert im Falle eines vorliegenden Folgeantrags/Zweitantrags, ob ein Verfahren durchgeführt wird und enthält in diesem Zusammenhang Aussagen zu Entscheidungen gem. § 51 Abs. 1 bis Abs. 3 VwVfG und § 60 Abs. 5 und 7 AufenthG.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="prognoseart">
                     <xs:annotation>
                        <xs:documentation>Diese Angabe trifft eine Aussage darüber, zu welcher Verfahrensart eine Prognosemeldung abgegeben wird.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:choice>
                           <xs:element name="prognoseErstverfahren">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element werden die Daten zur Prognose im Asylerstverfahren übermittelt.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="kurzfristigeEntscheidung" type="xs:boolean">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird übermittelt, ob eine kurzfristige Entscheidung zu unzulässig/o.u. (§ 50 AsylG) für die Antragsteller im betroffenen Verfahren getroffen werden kann.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="prognoseFolgeverfahren">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element werden die Daten für eine Prognose zum Folgeantrag/Zweitantrag übermittelt.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="prognosemeldung" type="xauslaender:Code.Asyl.PrognoseFolgantrag">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird die Prognose zur Asylentscheidung im Folgeantragsverfahren mitgeteilt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="bescheiddatum" minOccurs="0" type="xida:Tagesdatum">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird das Datum der Bescheiderstellung übermittelt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="anlageBescheid" minOccurs="0" type="xs:base64Binary">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird das Bescheiddokument übermittelt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="anlageRechtsbehelfsbelehrung"
                                                minOccurs="0"
                                                type="xs:base64Binary">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird das Dokument Rechtsbehelfsbelehrung zum Bescheid übermittelt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="foermlicherBescheidAnAntragsteller" type="xs:boolean">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element wird mitgeteilt, ob der/die Ausländer noch einen förmlichen Bescheid erhält/erhalten.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ausweisdokument"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="xauslaender:Ausweisdokument">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element werden Ausweisdokumente, welche sich im BAMF befinden, aufgeführt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="sonstigeUnterlagen"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="xauslaender:Asyl.SonstigeUnterlagen">
                                       <xs:annotation>
                                          <xs:documentation>Mit diesem Element werden Informationen zu sonstigen Unterlagen übermittelt.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:choice>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Entscheidungsuebermittlung.110202">
      <xs:annotation>
         <xs:appinfo>
            <title>Entscheidungsübermittlung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden Informationen zum Bescheid sowie der Bescheid selbst vom bamf an die zuständige abh (ABH/ZAB) und/oder ae (AE) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheid">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zum Bescheid übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="bescheiddatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Datum der Bescheiderstellung übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageBescheid" type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Bescheiddokument übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageRechtsbehelfsbelehrung" type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Dokument Rechtsbehelfsbelehrung zum Bescheid übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageAnhoerungsprotokoll"
                                       minOccurs="0"
                                       maxOccurs="unbounded"
                                       type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Anhörungsprotokoll übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageBescheiduebersetzung"
                                       minOccurs="0"
                                       type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das MARiS-PDF Bescheidübersetzung übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="sonstigeUnterlagen"
                              minOccurs="0"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.SonstigeUnterlagen">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zu sonstigen Unterlagen übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.BescheidZustelldatum.110203">
      <xs:annotation>
         <xs:appinfo>
            <title>Übermittlung Zustelldatum Bescheid</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird das Zustelldatum des Bescheids vom bamf an die zuständige abh (ABH/ZAB) übermittelt. Die zuständige abh kann erst ausländerrechtlich handeln, wenn der Bescheid wirksam, also zugegangen ist (§ 2 Abs. 1 und 2 VwZG i. V. m. § 10 Abs. 1 Nr. 1 VwZG).</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheiddatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Bescheiderstellung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheidZustelldatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält den Tag der Zustellung des Bescheids beim Antragsteller. Zustellung ist die Bekanntgabe eines schriftlichen oder elektronischen Dokuments. Sie wird durch einen Erbringer von Postdienstleistungen (Post) oder durch die Behörde ausgeführt (§ 2 Abs. 1 und 2 VwZG). Die Zustellung kann außerdem durch öffentliche Bekanntmachung erfolgen, wenn der Aufenthaltsort des Empfängers unbekannt ist und eine Zustellung an einen Vertreter oder Zustellbevollmächtigten nicht möglich ist (§ 10 Abs. 1 Nr. 1 VwZG).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Abschlussmitteilung.110204">
      <xs:annotation>
         <xs:appinfo>
            <title>Abschlussmitteilung Asylverfahren</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird der bestands- oder rechtskräftige Abschluss des Asylverfahrens vom bamf an die zuständige abh (ABH/ZAB) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheiddatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Erstellung desjenigen Bescheids übermittelt, auf den sich die vorliegende Mitteilung zur Bestands- oder Rechtskraft bezieht.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="abschlusssachverhalt"
                              maxOccurs="unbounded"
                              type="xauslaender:Code.Asyl.Abschlusssachverhalt">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Sachverhalt zum Abschluss des Asylverfahrens übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bestandsRechtskraftdatum"
                              minOccurs="0"
                              type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Bestands- oder Rechtskraftdatum zum Asylverfahren übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="gerichtsverfahren" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Angaben zu einem Gerichtsverfahren übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="zustaendigesVerwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird die Bezeichnung des zuständigen Verwaltungsgerichtes übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichenVerwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen des anhängigen Gerichtsverfahrens beim Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="gerichtsentscheidungsDatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Datum des Gerichtsbescheids/-beschlusses/-urteils übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="gerichtsverfahrenEingestellt" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird übermittelt, ob das Gerichtsverfahren eingestellt wurde.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageUrteilEntscheid" type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der/das Bescheid/Beschluss/Urteil des Gerichts als Dokument übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="abschiebungsandrohungVollziehbarkeitsdatum"
                              minOccurs="0"
                              type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Vollziehbarkeitsdatum einer bestehenden Abschiebungsandrohung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="antragAufZulassungBerufungAbgelehntEingestellt" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird mitgeteilt, ob es zum Übermittlungszeitpunkt einen Antrag auf Zulassung einer Berufung gegeben hat, welcher inzwischen abgelehnt oder eingestellt wurde (true) oder es keinen Antrag auf Zulassung einer Berufung im Vorfeld gab (false).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="verweisAufTeilabschlussmitteilung"
                              minOccurs="0"
                              type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird im Falle eines abgeschlossenen Verfahrens das Datum eines zuvor bereits mitgeteilten Teilabschlusses übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="abschiebungsanordnungBestandsRechtskraftdatum"
                              minOccurs="0"
                              type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Bestands- oder Rechtskraftdatum einer bestehenden Abschiebungsanordnung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheidZustelldatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält den Tag der Zustellung des Bescheids beim Antragsteller. Zustellung ist die Bekanntgabe eines schriftlichen oder elektronischen Dokuments. Sie wird durch einen Erbringer von Postdienstleistungen (Post) oder durch die Behörde ausgeführt (§ 2 Abs. 1 und 2 VwZG). Die Zustellung kann außerdem durch öffentliche Bekanntmachung erfolgen, wenn der Aufenthaltsort des Empfängers unbekannt ist und eine Zustellung an einen Vertreter oder Zustellbevollmächtigten nicht möglich ist (§ 10 Abs. 1 Nr. 1 VwZG).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="zustellnachweisBescheid" minOccurs="0" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Nachweis der Zustellung des Bescheides übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Teilabschlussmitteilung.110205">
      <xs:annotation>
         <xs:appinfo>
            <title>Teilabschlussmitteilung Asylverfahren</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird der bestands- oder rechtskräftige Teilabschluss des Asylverfahrens vom bamf an die zuständige abh (ABH/ZAB) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheiddatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Erstellung desjenigen Bescheides übermittelt, auf den sich die vorliegende Mitteilung zur Teilbestands- oder Teilrechtskraft bezieht.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="teilabschlusssachverhalt"
                              maxOccurs="unbounded"
                              type="xauslaender:Code.Asyl.Teilabschlusssachverhalt">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Sachverhalt zum Teilabschluss des Asylverfahrens übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="teilBestandsRechtskraftdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Teilbestands- oder -rechtskraftdatum zum Asylverfahren übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="gerichtsverfahren" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Angaben zu einem Gerichtsverfahren übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="zustaendigesVerwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird die Bezeichnung des zuständigen Verwaltungsgerichtes übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichenVerwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen des anhängigen Gerichtsverfahrens beim Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="gerichtsentscheidungsDatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Datum des Gerichtsbescheids/-beschlusses/-urteils übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="gerichtsverfahrenEingestellt" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird übermittelt, ob das Gerichtsverfahren eingestellt wurde.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="anlageUrteilEntscheid" type="xs:base64Binary">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der/das Bescheid/Beschluss/Urteil des Gerichts als Dokument übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="verfahrenSubsidiaererSchutz" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Diese Element gibt an, dass ein Verfahren zum subsidiären Schutz bzw. zu Abschiebungsverboten nach § 60 Abs. 5 oder 7 AufenthG anhängig ist.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="zustaendigesVerwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das zuständige Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichenVerwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen des anhängigen Gerichtsverfahrens beim Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="verfahren16aFluechtlingseigenschaft" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Dieses Element gibt an, dass ein Verfahren zu Art. 16a GG (ggfs. auch zur Zuerkennung der Flüchtlingseigenschaft) anhängig ist.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="zustaendigesVerwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das zuständige Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichenVerwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen des anhängigen Gerichtsverfahrens beim Verwaltungsgericht übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="antragAufZulassungBerufungAbgelehntEingestellt" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird mitgeteilt, ob es zum Übermittlungszeitpunkt einen Antrag auf Zulassung einer Berufung gegeben hat, welcher inzwischen abgelehnt oder eingestellt wurde (true) oder es keinen Antrag auf Zulassung einer Berufung im Vorfeld gab (false).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="verweisAufTeilabschlussmitteilung"
                              minOccurs="0"
                              type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird im Falle eines abgeschlossenen Verfahrens das Datum des letzten zuvor bereits mitgeteilten Teilabschlusses übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheidZustelldatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält den Tag der Zustellung des Bescheids beim Antragsteller. Zustellung ist die Bekanntgabe eines schriftlichen oder elektronischen Dokuments. Sie wird durch einen Erbringer von Postdienstleistungen (Post) oder durch die Behörde ausgeführt (§ 2 Abs. 1 und 2 VwZG). Die Zustellung kann außerdem durch öffentliche Bekanntmachung erfolgen, wenn der Aufenthaltsort des Empfängers unbekannt ist und eine Zustellung an einen Vertreter oder Zustellbevollmächtigten nicht möglich ist (§ 10 Abs. 1 Nr. 1 VwZG).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="zustellnachweisBescheid" minOccurs="0" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Nachweis der Zustellung des Bescheides übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Datenberichtigung.110206">
      <xs:annotation>
         <xs:appinfo>
            <title>Berichtigung von Personendaten vom BAMF</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird der zuständigen abh (ABH/ZAB) und ae (AE) mitgeteilt, dass das Bundesamt in Zusammenhang mit der Asylantragstellung oder der Anhörung zum Asylantrag Informationen erhalten hat, aufgrund derer die bisher verwendeten Personendaten in MARiS berichtigt wurden.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die alten Daten der Person übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="aenderungsanlass"
                              type="xauslaender:Code.Asyl.UebermittlungsanlassDatenpruefung">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Grund für die Änderung der Personendaten übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="grunddatenNeu" type="xauslaender:Asyl.PersonGrunddaten">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die neuen Grunddaten zu der anhand der Angaben im Element person identifizierten Person vollständig übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.PTUBericht.110207">
      <xs:annotation>
         <xs:appinfo>
            <title>Übermittlung PTU-Bericht</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden die Ergebnisse einer, von der im Bundesamt für die physikalisch-technische Urkundenuntersuchung (PTU) zuständigen Stelle durchgeführten Dokumentenprüfung übermittelt. Die Nachricht wird vom bamf an die zuständige abh (ABH/ZAB) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anlagePtuBericht"
                              maxOccurs="unbounded"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der/die Untersuchungsbericht/-e übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.EinstellungVorAntragstellung.110208">
      <xs:annotation>
         <xs:appinfo>
            <title>Einstellung des Verfahrens vor Antragstellung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird die Information übermittelt, dass die Bearbeitung des Asylgesuchs (Verfahrens) wegen Nichtbetreibens als eingestellt gilt. Die Nachricht wird vom bamf an die zuständige abh (ABH/ZAB) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="einstellungsgrund" type="xauslaender:Code.Asyl.Einstellungsgrund">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Grund für die Einstellung des Verfahrens übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="einstellungsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Einstellung des Verfahrens für die Fälle übermittelt, in denen der Asylsuchende nicht in der zuständigen Aufnahmeeinrichtung erscheint.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Mehrfachvorgaenge.110209">
      <xs:annotation>
         <xs:appinfo>
            <title>Mehrfachvorgänge bzw. Personengleichheit</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden Erkenntnisse zu Mehrfachvorgängen bzw. Personengleichheit vom bamf an die zuständige abh (ABH/ZAB) und/oder ae (AE) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Person übermittelt, die unter einem BAMF-Aktenzeichen geführt wird und auf die sich die Mehrfachvorgänge beziehen.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="vorgang" maxOccurs="unbounded">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die betroffenen Vorgänge aufgeführt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:extension base="xauslaender:Asyl.PersonGrunddaten">
                              <xs:sequence>
                                 <xs:element name="bAMFAktenzeichen" type="xauslaender:Asyl.BAMFAktenzeichen">
                                    <xs:annotation>
                                       <xs:documentation>Mit diesem Element wird das BAMF-Aktenzeichen des betroffenen Vorgangs übermittel.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="zustaendigeABH" type="xauslaender:Behoerde">
                                    <xs:annotation>
                                       <xs:documentation>Mit diesem Element wird die für diesen Vorgang zuständige abh übermittelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="istUrStammverfahren"
                                             minOccurs="0"
                                             fixed="true"
                                             type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn es sich hierbei um das Ur- bzw. Stammverfahren handelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="istMehrfachantrag"
                                             minOccurs="0"
                                             fixed="true"
                                             type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn es sich hierbei um einen Mehrfachantrag handelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="istVerdeckterFolgeantrag"
                                             minOccurs="0"
                                             fixed="true"
                                             type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn es sich hierbei um einen verdeckten Folgeantrag handelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="nichtEntschieden" minOccurs="0">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element wird übermittelt, wenn über den Vorgang noch nicht entschieden wurde.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element name="ergehtEntscheidung"
                                                      minOccurs="0"
                                                      fixed="true"
                                                      type="xs:boolean">
                                             <xs:annotation>
                                                <xs:documentation>Dieses Element ist zu übermitteln, wenn zu dem Verfahren noch eine Entscheidung ergeht.</xs:documentation>
                                             </xs:annotation>
                                          </xs:element>
                                          <xs:element name="keineWeiterbearbeitung"
                                                      minOccurs="0"
                                                      fixed="true"
                                                      type="xs:boolean">
                                             <xs:annotation>
                                                <xs:documentation>Dieses Element ist zu übermitteln, wenn das Verfahren nicht weiter bearbeitet wird.</xs:documentation>
                                             </xs:annotation>
                                          </xs:element>
                                       </xs:sequence>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element name="entschieden" minOccurs="0">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element wird übermittelt, wenn über den Vorgang entschieden wurde.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element name="bescheidZustelldatum" minOccurs="0" type="xida:Tagesdatum">
                                             <xs:annotation>
                                                <xs:documentation>Dieses Element ist zu übermitteln, wenn ein Bescheid bereits zugestellt wurde. Es enthält den Tag der Zustellung des Bescheids beim Antragsteller. Der Bescheid ist damit noch nicht bestandskräftig. Zustellung ist die Bekanntgabe eines schriftlichen oder elektronischen Dokuments. Sie wird durch einen Erbringer von Postdienstleistungen (Post) oder durch die Behörde ausgeführt (§ 2 Abs. 1 und 2 VwZG). Die Zustellung kann außerdem durch öffentliche Bekanntmachung erfolgen, wenn der Aufenthaltsort des Empfängers unbekannt ist und eine Zustellung an einen Vertreter oder Zustellbevollmächtigten nicht möglich ist (§ 10 Abs. 1 Nr. 1 VwZG).</xs:documentation>
                                             </xs:annotation>
                                          </xs:element>
                                          <xs:element name="klageEingereicht"
                                                      minOccurs="0"
                                                      fixed="true"
                                                      type="xs:boolean">
                                             <xs:annotation>
                                                <xs:documentation>Dieses Element ist zu übermitteln, wenn zu dem Verfahren eine Klage eingereicht wurde.</xs:documentation>
                                             </xs:annotation>
                                          </xs:element>
                                          <xs:element name="antragEingereicht"
                                                      minOccurs="0"
                                                      fixed="true"
                                                      type="xs:boolean">
                                             <xs:annotation>
                                                <xs:documentation>Dieses Element ist zu übermitteln, wenn zu dem Verfahren ein Antrag eingereicht wurde.</xs:documentation>
                                             </xs:annotation>
                                          </xs:element>
                                       </xs:sequence>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element name="istRechtsBestandskraeftig"
                                             minOccurs="0"
                                             fixed="true"
                                             type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn dieses Verfahren bereits bestands-/rechtskräftig abgeschlossen ist.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="loeschungVeranlasst"
                                             minOccurs="0"
                                             fixed="true"
                                             type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn die Löschung des Datensatzes im AZR veranlasst wurde.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="weitereBearbeitungInAussenstelle"
                                             minOccurs="0"
                                             type="xauslaender:Asyl.AussenstelleBAMF">
                                    <xs:annotation>
                                       <xs:documentation>Mit diesem Element wird die für die weitere Bearbeitung zuständige BAMF-Außenstelle übermittelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="verbleib" minOccurs="0" type="xoev-lc:String.Latin">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn Angaben zum Verbleib der betreffenden Person gemacht werden können.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element name="sonstigeInformationen"
                                             minOccurs="0"
                                             type="xoev-lc:String.Latin">
                                    <xs:annotation>
                                       <xs:documentation>Dieses Element ist zu übermitteln, wenn sonstige Information zu diesem Vorgang vorliegen.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                              </xs:sequence>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Zweitantrag.110210">
      <xs:annotation>
         <xs:appinfo>
            <title>Zweitantrag</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht informiert das bamf die zuständige abh (ABH/ZAB), dass der Asylantrag des Ausländers die Eigenschaft als Zweitantrag § 71a Abs. 1 AsylG erfüllt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Gerichtsverfahren.110301">
      <xs:annotation>
         <xs:appinfo>
            <title>Gerichtsverfahren und/oder Antrag</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht werden Informationen zu einem gestellten Antrag und/oder einer eingereichten Klage vom bamf an die zuständige abh (ABH/ZAB) übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="zustaendigesVerwaltungsgericht" type="xoev-lc:String.Latin">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das zuständige Verwaltungsgericht übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="klage" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält Angaben zu einem anhängigen Klageverfahren.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="aktenzeichenKlageVerwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen zu einer beim zuständigen Verwaltungsgericht eingereichten Klage übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="klageeingangsdatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Dieses Element gibt das Klageeingangsdatum an.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="antrag805" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zu einem Antrag nach § 80 Abs. 5 VwGO (Aufschiebende Wirkung) übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="vollziehbarkeitsdatum" minOccurs="0" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Dieses Element gibt das Datum der Vollziehbarkeit zur Entscheidung über die Abschiebungsandrohung bzw.Abschiebungsanordnung an.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichen805Verwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen zu einem gem. § 80 Abs. 5 VwGO beim zuständigen Verwaltungsgericht gestellten Antrag übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="antragsdatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Datum der Antragsstellung bei Gericht zu einem Antrag gem. § 80 Abs. 5 VwGO übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="beschluss" minOccurs="0" type="xauslaender:Asyl.Beschluss">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der Beschluss zu einem Antrag nach § 80 Abs. 5 VwGO übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aufschiebendeWirkungAngeordnetAm"
                                       minOccurs="0"
                                       type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Beschlussdatum übermittelt, mit dem die aufschiebende Wirkung hinsichtlich der Abschiebung nur in den betreffenden Staat wegen eines Abschiebungsverbotes nach § 60 Abs. 5 bzw. 7 AufenthG angeordnet wird. Das bamf führt das Asylverfahren nicht fort.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="antrag123" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zu einem Antrag nach § 123 VwGO (Erlass einstweiliger Anordnungen) übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="aktenzeichen123Verwaltungsgericht" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen zu einem gem. § 123 VwGO beim zuständigen Verwaltungsgericht gestellten Antrag übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="antragsdatum" type="xida:Tagesdatum">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Datum der Antragsstellung bei Gericht zu einem Antrag gem. § 123 VwGO übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="beschluss" minOccurs="0" type="xauslaender:Asyl.Beschluss">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der Beschluss zu einem Antrag nach § 123 VwGO übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="wiedereinsetzungsantrag" minOccurs="0">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird mitgeteilt, ob ein Wiedereinsetzungsantrag gegen die Versäumung der Klagefrist oder der Antragsfrist nach § 80 Abs. 5 VwGO gestellt wurde.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="aktenzeichenKlageVerwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen zu einer beim zuständigen Verwaltungsgericht eingereichten Klage übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="aktenzeichen805Verwaltungsgericht"
                                       minOccurs="0"
                                       type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird das Aktenzeichen zu einem gem. § 80 Abs. 5 VwGO beim zuständigen Verwaltungsgericht gestellten Antrag übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="klagefrist" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Dieses Element gibt an, dass ein Wiedereinsetzungantrag gegen die Versäumung der Klagefrist gestellt wurde.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="antragsfrist805" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Dieses Element gibt an, dass ein Wiedereinsetzungantrag gegen die Versäumung der Antragsfrist nach § 80 Abs. 5 VwGO gestellt wurde. Eine Entscheidung des Gerichts hierüber ist bislang nicht erfolgt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Einzelfallinformation.110501">
      <xs:annotation>
         <xs:appinfo>
            <title>Einzelfallinformation vom BAMF</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht kann das bamf der zuständigen abh (ABH/ZAB) und ae (AE) Informationen übermitteln, welche nicht über eine der strukturierten anderen Nachrichten abgebildet werden.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.BasisnachrichtVonBAMF">
               <xs:sequence>
                  <xs:element name="person" maxOccurs="unbounded" type="xauslaender:Asyl.Person">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anlageEinzelfallinformation"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird ein Dokument zum besonderen Einzelfall übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="betreff" type="xoev-lc:String.Latin">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Thema des speziellen Einzelfalls übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="freitext" minOccurs="0" type="xoev-lc:String.Latin">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zu einem besonderen Einzelfall in Textform übermittelt, welche nicht über eine der strukturierten anderen Nachrichten abgebildet werden können.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.AnzeigeGeburt.111101">
      <xs:annotation>
         <xs:appinfo>
            <title>Anzeige Geburt oder Einreise eines Kindes</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilt die zuständige abh (ABH/ZAB) dem bamf mit, dass ein Kind geboren wurde bzw. eingereist ist, für das die Voraussetzungen für eine Asylantragstellung nach § 14 a Abs. 2 AsylG vorliegen. Diese Nachricht ist vom bamf als Antragsfiktion für das Kind zu bewerten.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die Personengrunddaten des Kindes übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="elternteil" maxOccurs="2">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird ein sorgeberechtigter Elternteil des Kindes übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:extension base="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                              <xs:sequence>
                                 <xs:element name="artDesAufenthaltsstatus"
                                             type="xauslaender:Code.Art.des.Aufenthaltsstatus">
                                    <xs:annotation>
                                       <xs:documentation>In diesem Element wird der aktuelle Aufenthaltsstatus des sorgeberechtigten Elternteils übermittelt.</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                              </xs:sequence>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="anlage" maxOccurs="unbounded" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält den Nachweis der Geburt (Auszug aus dem Geburtenregister oder Urkunde) eines Kindes und/oder Dokumente zur Einreise.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.WiederzuzugAZR.111201">
      <xs:annotation>
         <xs:appinfo>
            <title>Gemeldeter Wiederzuzug im AZR</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilen die zuständige abh (ABH/ZAB) oder ae (AE) dem BAMF mit, dass nach einer Folgeantragstellung der Wiederzuzug der unter dem BAMF-Aktenzeichen geführten Personen im AZR gemeldet wurde.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anschrift" minOccurs="0" type="xima:Meldeanschrift">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die aktuelle Anschrift des Ausländers übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.VollzugAusreise.111202">
      <xs:annotation>
         <xs:appinfo>
            <title>Vollzug der Abschiebungsandrohung/-anordnung</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilt die zuständige abh (ABH/ZAB) dem bamf mit, dass die Abschiebung/Ausreise vollzogen wurde.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Person übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="vollzugsdatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Das Element gibt das Datum der freiwilligen Ausreise oder der Abschiebung an.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="abgeschoben" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Das Element gibt an, ob der Ausländer abgeschoben wurde. Wurde der Ausländer nicht abgeschoben, ist er freiwillig ausgereist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="zielstaat" minOccurs="0" type="xauslaender:AngabenZumStaat">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Staat übermittelt in den der Ausländer nach Abschluss des Verfahrens ausreist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anlageAusreisebestaetigung"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird ein Dokument zur Bestätigung der Ausreise übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="artDerAusreise">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird mitgeteilt, ob die Ausreise nachgewiesen ist oder freiwillig erfolgte.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="nachgewieseneAusreise" fixed="true" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird mitgeteilt, dass die Ausreise nachgewiesen ist.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="vermuteteFreiwilligeAusreise" fixed="true" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird mitgeteilt, dass die Ausreise vermutlich freiwillig erfolgt ist, da kein gesonderter Nachweis vorliegt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Meldeanschrift.111205">
      <xs:annotation>
         <xs:appinfo>
            <title>Geänderte Meldeanschrift</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht wird eine geänderte Meldeanschrift von der zuständigen abh (ABH/ZAB) oder ae (AE) an das bamf übermittelt.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, für die eine geänderte Meldeanschrift übermittelt wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="alteMeldeanschrift" type="xima:Meldeanschrift">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die bisherige Meldeanschrift übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="neueMeldeanschrift" type="xima:Meldeanschrift">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die neue Meldeanschrift übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.Zustaendigkeitserklaerung.111206">
      <xs:annotation>
         <xs:appinfo>
            <title>Geänderte Zuständigkeit</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht meldet eine abh (ABH/ZAB) ihre Zuständigkeit an das bamf.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die Grunddaten der Person übermittelt, für die von der abh die Zuständigkeit übernommen wurde.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.NichterscheinenInAE.111207">
      <xs:annotation>
         <xs:appinfo>
            <title>Nichterscheinen in AE</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilt die zuständige ae (AE) dem bamf mit, dass der Ausländer nach der Weiterleitung bzw. Verteilung nicht erschienen ist.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Nachricht.G2G.XA">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden die Personengrunddaten des Ausländers übermittelt, der nicht in der ae erschienen ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="belehrungZurWeiterleitung"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die Kopie der unterschriebenen Belehrung zur Weiterleitung übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="aufforderungZurMeldungInAE"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Nachweis der zuständigen ae übermittelt, dass sie die asylsuchende Person aufgefordert hat in der Aufnahmeeinrichtung zu erscheinen.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="buema" minOccurs="0" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die Bescheinigung über die Meldung als Asylsuchender (BüMA) übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="einbehalteneDokumente" minOccurs="0" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die Bescheinigung über einbehaltene Dokumente übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.RuecknahmeAsylantrag.111208">
      <xs:annotation>
         <xs:appinfo>
            <title>Rücknahme eines Asylantrages</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilt die zuständige abh (ABH/ZAB) oder ae (AE) dem bamf mit, dass der Ausländer die Rücknahme des Asylantrages schriftlich erklärt hat.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden und ihren Asylantrag zurückgenommen haben.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="schriftlicheRuecknahme" type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird die Kopie der vom Antragsteller oder den Antragstellenden unterschriebenen Bekundung der Rücknahme des Asylantrags übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="ruecknahmedatum" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum der Rücknahme mitgeteilt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="grund">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird der Grund der Rücknahme des Asylantrags übermittelt.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:choice>
                           <xs:element name="grundDerRuecknahme" type="xauslaender:Code.Asyl.Ruecknahmegrund">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der Grund der Rücknahme des Asylantrags übermittelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="sonstigerGrund" type="xoev-lc:String.Latin">
                              <xs:annotation>
                                 <xs:documentation>Mit diesem Element wird der Grund der Rücknahme des Asylantrags übermittelt, sofern es sich nicht um eine freiwillige Ausreise oder die Erteilung eines Aufenthaltstitels handelt.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:choice>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.FortzugNachUnbekannt.111209">
      <xs:annotation>
         <xs:appinfo>
            <title>Fortzug nach Unbekannt</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht teilt die zuständige abh (ABH/ZAB) oder ae (AE) dem bamf mit, dass der Ausländer unter seiner Meldeanschrift dauerhaft nicht erreichbar ist.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person" type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Person übermittelt, die unter einem BAMF-Aktenzeichen geführt wird.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="fortzugNachUnbekanntAm" type="xida:Tagesdatum">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Datum mitgeteilt, zu dem nach Kenntnis der Ausländerbehörde der Fortzug nach Unbekannt eingetreten ist (Siehe § 3 Abs. 1 Nummer 6 e) AZRG-DV).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="Asyl.EinzelfallinformationAnBAMF.111501">
      <xs:annotation>
         <xs:appinfo>
            <title>Einzelfallinformation an BAMF</title>
         </xs:appinfo>
         <xs:documentation>Mit dieser Nachricht kann die zuständige abh (ABH/ZAB) oder ae (AE) Informationen an das bamf übermitteln, welche nicht über eine der strukturierten anderen Nachrichten abgebildet werden.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="xauslaender:Asyl.Basisnachricht">
               <xs:sequence>
                  <xs:element name="person"
                              maxOccurs="unbounded"
                              type="xauslaender:Asyl.PersonGrunddatenMitAZRNummer">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Daten der Personen übermittelt, die unter einem BAMF-Aktenzeichen geführt werden und vom Inhalt dieser Nachricht betroffen sind.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anlageEinzelfallinformation"
                              minOccurs="0"
                              type="xs:base64Binary">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird ein Dokument zum besonderen Einzelfall übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="betreff" type="xoev-lc:String.Latin">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element wird das Thema des speziellen Einzelfalls übermittelt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="freitext" minOccurs="0" type="xoev-lc:String.Latin">
                     <xs:annotation>
                        <xs:documentation>Mit diesem Element werden Informationen zu einem besonderen Einzelfall in Textform übermittelt, welche nicht über eine der strukturierten anderen Nachrichten abgebildet werden können.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
</xs:schema>
