         <xs:element name="letzteAenderung" minOccurs="0" type="xida:Tagesdatum">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird das Datum des letzten schreibenden Zugriffs übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="referenz.id" type="xoev-lc:String.Latin">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird die Referenz-ID zur Identifizierung des betreffenden Sachverhalts innerhalb des Datensatzes übermittelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
