                     <xs:annotation>
                        <xs:documentation>Hier wird die Mitteilung, ob es sich um einen Auslandssterbefall handelt, berichtigt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="sterbestaat" minOccurs="0" type="xpersonenstand:Code.Staat">
