                     </xs:annotation>
                  </xs:element>
                  <xs:element name="begruendungOrt" type="xpersonenstand:Ereignisort">
                     <xs:annotation>
                        <xs:documentation>Ort der Begründung der Lebenspartnerschaft</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="begruendungDatum" type="xida:TeilbekanntesDatumMitUnbekannt">
                     <xs:annotation>
                        <xs:documentation>Datum der Begründung der Lebenspartnerschaft</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
