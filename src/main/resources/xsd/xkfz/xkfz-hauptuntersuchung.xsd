  <xs:complexType name="Type.MitteilungUntersuchungZFZR">
    <xs:annotation>
      <xs:documentation>Datentyp zur Übermittlung von Daten zu einer Hauptuntersuchung oder Sicherheitsprüfung an das Zentrale Fahrzeugregister (ZFZR).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="identifizierungFahrzeug" type="xkfz:Type.IdentifizierungFahrzeug">
        <xs:annotation>
          <xs:documentation>Fahrzeug beschreibende Daten, welche bei einer HU/SP erfasst wurden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="beschreibungFahrzeug" minOccurs="0" type="xkfz:Type.BeschreibungFahrzeug">
        <xs:annotation>
          <xs:documentation>Daten, welche das betroffene Fahrzeug näher beschreiben</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="identifizierungUntersuchung" type="xkfz:Type.IdentifizierungUntersuchung">
        <xs:annotation>
          <xs:documentation>Daten, welche die betroffene Untersuchung bzw. Prüfung identifizieren</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ergebnisUntersuchung" type="xkfz:Type.ErgebnisUntersuchung">
        <xs:annotation>
          <xs:documentation>Ergebnisse und Messwerte der Untersuchung bzw. Prüfung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="listeMaengel" minOccurs="0" type="xkfz:Type.ListeMaengel">
        <xs:annotation>
          <xs:documentation>Liste über die bei einer Hauptuntersuchung festgestellten Mängel gem. Mangelbaum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="listeMonetaereEinnahmen" minOccurs="0" type="xkfz:Type.ListeMonetaereEinnahmen">
        <xs:annotation>
          <xs:documentation>Liste über die Entgelte und Gebühren</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="listeHinweise" minOccurs="0" type="xkfz:Type.ListeHinweise">
        <xs:annotation>
          <xs:documentation>Liste zur Übermittlung von weiteren Hinweisen über die in der Zukunft durch Verschleiß, Korrosion oder andere Umstände sich abzeichnende Mängel.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.MitteilungUntersuchungZS">
    <xs:annotation>
      <xs:documentation>Datentyp zur Übermittlung von Daten zu einer Hauptuntersuchung oder Sicherheitsprüfung an die Zentrale Stelle (ZS)</xs:documentation>
