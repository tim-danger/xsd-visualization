  <xs:complexType name="Type.AnfrageUntersuchungZFZR">
    <xs:annotation>
      <xs:documentation>Datentyp zur Anfrage von Daten zu einer Hauptuntersuchung oder Sicherheitsprüfung an das Zentrale Fahrzeugregister (ZFZR)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="zusatzprotokollierung" type="auskunft:ZusatzprotokollierungAnlass0">
        <xs:annotation>
          <xs:documentation>Daten der Zusatzprotokollierung gem. § 41 FZV.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="schluesselnummerUeberwachungsinstitutionOderZDK" type="xkfz:Code.SchluesselnummerDerUeberwachungsinstitutionOderZDK">
        <xs:annotation>
          <xs:documentation>Schluesselnummer der Überwachungsinstitution bzw. Gemeinschaftseinrichtung der anerkannten Werkstätten, bei welcher die Untersuchung/Prüfung durchgeführt wurde, gem. XKfz-Codeliste.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:KontrollnummerSP" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Kontrollnummer der anerkannten Kraftfahrzeugwerkstatt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="artDerUntersuchung" type="xkfz:Code.ArtDerUntersuchung">
        <xs:annotation>
          <xs:documentation>Art der technischen Untersuchung (Hauptuntersuchung oder Sicherheitsprüfung). Code der Schlüsseltabelle Art der Untersuchung.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fahrzeugidentifizierungsnummer" type="typ:typ-Fahrzeugidentifizierungsnummer">
        <xs:annotation>
          <xs:documentation>Fahrzeug-Identifizierungsnummer des untersuchten Fahrzeugs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kennzeichen" minOccurs="0" type="typ:typ-KennzeichenHUSP">
        <xs:annotation>
          <xs:documentation>Kennzeichen des untersuchten Fahrzeugs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:Berichtsnummer" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Nummer des Untersuchungsberichts bzw. Prüfprotokolls.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="Type.Anfragedaten">
    <xs:annotation>
      <xs:documentation>Datentyp zur Übermittlung der Daten der ursprünglichen Anfrage in einer Auskunft</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="500" />
      <xs:minLength value="1" />
      <xs:pattern value="[\p{IsBasicLatin}\p{IsLatin-1Supplement}-[\t\r\n&#x7F;&#x80;-&#xA0;]]+" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Type.AuskunftDetails">
    <xs:annotation>
      <xs:documentation>Datentyp zur Übermittlung von Details zur Auskunft</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="anfragedaten" type="xkfz:Type.Anfragedaten">
        <xs:annotation>
          <xs:documentation>Die Daten der Anfrage, einschließlich der Daten zur Zusatzprotokollierung werden zur Kontrolle in Zeilenform, jeweils getrennt durch Semikolon, übermittelt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.AuskunftUntersuchungZFZR">
    <xs:annotation>
      <xs:documentation>Datentyp zur Auskunft über eine Hauptuntersuchung oder Sicherheitsprüfung aus dem Zentralen Fahrzeugregister (ZFZR)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="auskunftDetails" type="xkfz:Type.AuskunftDetails">
        <xs:annotation>
          <xs:documentation>Details zur Auskunft. Anfragedaten.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="untersuchungsabschnitt" type="xkfz:Type.Untersuchungsabschnitt">
        <xs:annotation>
          <xs:documentation>Untersuchungsabschnitt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.BasisDatumHauptuntersuchung">
    <xs:annotation>
      <xs:documentation>Basisdatenstruktur für die Anfrage und Übermittlung des (Fälligkeits-)Datums der nächsten Hauptuntersuchung. Es müssen stets die Schlüsseldaten (Fahrzeugidentifizierungsnummer, Herstellerschlüssel und Erstzulassungsdatum) angegeben werden. Die Angabe des Kennzeichens erfolgt optional.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="el:Fahrzeugidentifizierungsnummer">
        <xs:annotation>
          <xs:documentation>Fahrzeugidentifizierungsnummer (FIN)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:SchluesselHersteller">
        <xs:annotation>
          <xs:documentation>Herstellerschlüssel (HSN)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:DatumErstzulassung">
        <xs:annotation>
          <xs:documentation>Datum der ersten Zulassung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:Kennzeichen" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Kennzeichen</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:TextHersteller" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Text Hersteller</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.DatenPruefziffer">
    <xs:annotation>
      <xs:documentation>Datentyp zur Übermittlung einer HU-/SP-Prüfziffer sowie der Daten zu deren Generierung.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fahrzeugidentifizierungsnummer" type="xkfz:Type.Fahrzeugidentifizierungsnummer17">
        <xs:annotation>
          <xs:documentation>Fahrzeugidentifizierungsnummer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="monatJahrErstzulassung" type="xkfz:Type.MonatJahr">
        <xs:annotation>
          <xs:documentation>Jahr und Monat der Erstzulassung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="datumUntersuchung" type="xs:date">
        <xs:annotation>
          <xs:documentation>Datum an dem die Untersuchung/Prüfung durchgeführt wurde.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="merkmalBestanden" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Merkmal, ob die Untersuchung/Prüfung bestanden wurde:
true= bestanden
false= nicht bestanden</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:annotation>
          <xs:documentation>Struktur zur Unterscheidung der Datum der nächsten Untersuchung oder Prüfung.</xs:documentation>
        </xs:annotation>
        <xs:element ref="el:NaechsteHauptuntersuchung">
          <xs:annotation>
            <xs:documentation>Datum der nächsten Hauptuntersuchung</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="el:NaechsteSicherheitspruefung">
          <xs:annotation>
            <xs:documentation>Datum der nächsten Sicherheitsprüfung</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="schluesselnummerUeberwachungsinstitutionOderZDK" type="xkfz:Code.SchluesselnummerDerUeberwachungsinstitutionOderZDK">
        <xs:annotation>
          <xs:documentation>Schluesselnummer der Überwachungsinstitution bzw. Gemeinschaftseinrichtung der anerkannten Werkstätten, bei welcher die Untersuchung/Prüfung durchgeführt wurde, gem. XKfz-Codeliste.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pruefziffer" minOccurs="0" type="xkfz:Type.Pruefziffer">
        <xs:annotation>
          <xs:documentation>Inaktive (noch nicht produktiv genutzte) HU-/SP-Prüfziffer</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.DatumHauptuntersuchung">
    <xs:annotation>
      <xs:documentation>Datenstruktur zur Übermittlung des (Fälligkeits-)Datums der nächsten Hauptuntersuchung und ggf. auch der nächsten Sicherheitsprüfung</xs:documentation>
