              <xs:documentation>Identifikationsdokument,z.B. Ausweis, Reisepass oder Handelsregisterauszug</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
