          <xs:documentation>Zeitpunkt des Statusübergangs</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="referenzVeranlasser" minOccurs="0" type="xkfz:Type.ReferenzAntragsteller">
        <xs:annotation>
          <xs:documentation>Veranlasser des Statusübergangs</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.StatusuebergangAuskunft">
    <xs:annotation>
      <xs:documentation>Statusübergang des elektronischen Antrags auf Halterauskunft</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="status" type="xkfz:Code.StatusElektronischerAntragAuskunft">
        <xs:annotation>
          <xs:documentation>neuer Statuswert</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zeitpunktDesStatusuebergangs" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>Zeitpunkt zu dem der zugehörige Statuswert aktiv wurde</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.Steuerbefreiung">
    <xs:annotation>
      <xs:documentation>Angaben zum Typ der Steuerbefreiung. Ist ein Fahrzeug steuerbefreit, müssen diese Informationen im Rahmen Erfassung der Zulassungsdaten zur Verfügung gestellt werden.</xs:documentation>
    </xs:annotation>
