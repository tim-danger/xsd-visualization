      <xs:extension base="auskunft:FahrzeugbeschreibungZWTyp">
        <xs:sequence>
          <xs:element ref="el:SchluesselHersteller" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Herstellerschlüssel (HSN)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element ref="el:Fahrzeugidentifizierungsnummer" minOccurs="0">
