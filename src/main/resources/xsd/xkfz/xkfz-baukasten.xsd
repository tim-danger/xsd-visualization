          <xs:documentation>Aktenzeichen z.B. der Polizei oder des Rechtsanwalts</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="rechtsgrundlage" type="xkfz:Code.RechtsgrundlageHalterauskunft">
