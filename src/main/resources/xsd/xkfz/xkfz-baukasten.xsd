        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.Statusuebergang">
    <xs:annotation>
      <xs:documentation>Statusübergang des elektronischen Antrags. Ein Statusübergang besteht immer aus dem neuen Statuswert und dem Zeitpunkt des Übergangs.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="status" type="xkfz:Code.StatusElektronischerAntrag">
        <xs:annotation>
          <xs:documentation>Statuswert</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zeitpunktDesStatusuebergangs" type="xs:dateTime">
