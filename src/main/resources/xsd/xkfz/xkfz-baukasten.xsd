        </xs:annotation>
      </xs:element>
      <xs:element ref="el:TextTyp">
        <xs:annotation>
          <xs:documentation>Bezeichnung des Fahrzeugtyps</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:SchluesselTyp">
        <xs:annotation>
          <xs:documentation>Schlüsselnummer des Fahrzeugtyps</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="el:TextVariante" minOccurs="0">
